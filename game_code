import turtle
import time
import random

# Default Values
score = 0
high_score = 0
delay = 0.2 #For game speed. Harder the Level, Smaller the Delay

# Creating a window screen
wn = turtle.Screen()
wn.title("Snek Game")
wn.bgcolor("dark green")

# the width and height can be put as user's choice
wn.setup(width=900, height=700)
wn.tracer(0)

# head of the snake
head = turtle.Turtle() 
head.shape("circle")
head.color("black")

#FOR NEXT TIME
#How to commit Snek Game File
head.penup()
head.goto(0, 0)
head.direction = "Stop"

wn.mainloop()

